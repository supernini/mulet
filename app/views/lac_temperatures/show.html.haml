- model_class = LacTemperature
.page-header
  %h1=t '.title', :default => model_class.model_name.human

%p
  %strong= model_class.human_attribute_name(:lac_id) + ':'
  %br
  = @lac_temperature.lac_id
%p
  %strong= model_class.human_attribute_name(:when_happen) + ':'
  %br
  = @lac_temperature.when_happen
%p
  %strong= model_class.human_attribute_name(:temperature) + ':'
  %br
  = @lac_temperature.temperature

.form-actions
  = link_to t('.back', :default => t("helpers.links.back")), lac_temperatures_path, :class => 'btn'
  = link_to t('.edit', :default => t("helpers.links.edit")), edit_lac_temperature_path(@lac_temperature), :class => 'btn'
  = link_to t('.destroy', :default => t("helpers.links.destroy")), lac_temperature_path(@lac_temperature), :method => "delete", :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-danger'
